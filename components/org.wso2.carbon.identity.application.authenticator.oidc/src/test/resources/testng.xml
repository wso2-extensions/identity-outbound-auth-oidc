<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Carbon-Security-Test-Suite">

    <test name="oidc-connect-authenticator-tests" preserve-order="true" parallel="false">
        <classes>
            <class name="org.wso2.carbon.identity.application.authenticator.oidc.OpenIDConnectAuthenticatorTest"/>
        </classes>
    </test>
    <test name="oidc-connect-logout-processor-tests" preserve-order="true" parallel="false">
        <classes>
            <class name="org.wso2.carbon.identity.application.authenticator.oidc.processor.FederatedIdpInitLogoutProcessorTest"/>
            <class name="org.wso2.carbon.identity.application.authenticator.oidc.factory.LogoutRequestFactoryTest"/>
            <class name="org.wso2.carbon.identity.application.authenticator.oidc.factory.LogoutResponseFactoryTest"/>
        </classes>
    </test>
</suite>
